<script type="text/javascript">
window.onload=function(){	
var yuan=document.getElementById("yuan-button"),
	y_button=yuan.getElementsByTagName("li"),
	pic=document.getElementById("pic"),
	lButton=document.getElementById("left-button"),
	rButton=document.getElementById("right-button");
	var index=0;
	
function color(e){
	   for(var i=0;i<y_button.length;i++){
		  if(i!=e){
			  y_button[i].className="whcolor";
		  }
		  else{y_button[e].className="bkcolor";}
		}
	}	
	
for(var i=0;i<y_button.length;i++){
	y_button[i].onmouseover=function(){
		pic.style.left=parseInt(this.getAttribute("index")*-450)+"px";
		color(parseInt(this.getAttribute("index")));	 
		};	
	};  
	
rButton.onclick=function(){
	if(pic.style.left=="-2250px"||pic.style.left==""){
	  pic.style.left=0;
	  e=-(parseInt(pic.style.left)/450);
	  color(e);
	  }else {
	  pic.style.left=parseInt(pic.style.left)-450+"px";
	   e=-(parseInt(pic.style.left)/450);
	  color(e);}
	}	
	
lButton.onclick=function(){
	if(pic.style.left==0||pic.style.left==""){
	  pic.style.left="-2250px";
	   e=-(parseInt(pic.style.left)/450);
	  color(e);
	  }else{
	  pic.style.left=parseInt(pic.style.left)+450+"px";
	   e=-(parseInt(pic.style.left)/450);
	  color(e);}
	}	
}
</script>
<script src="myproductions/jQuery/jquery-1.12.3.min.js"></script>
<script>
$(function(){
$("#adv-button2").mouseenter(function(){
$("#adv1").animate({left:-1190});
$(this).css("background-color","orange");
$("#adv-button1").css("background-color","#666666");
	})
$("#adv-button1").mouseenter(function(){
$("#adv1").animate({left:0});
$(this).css("background-color","orange");
$("#adv-button2").css("background-color","#666666");
	})	
})
</script>
